{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/index.cjs"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
